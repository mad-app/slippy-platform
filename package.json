{
  "name": "slippy",
  "version": "0.1.0",
  "description": "Flexible and powerful toolkit for load and functional testing",
  "main": "./server.ts",
  "engines": {
    "node": ">= 8.6.0"
  },
  "scripts": {
    "start": "node node_modules/nodemon/bin/nodemon.js --config .nodemon.json server.ts",
    "test": "bash test/runner.sh && bash test/core/run.sh && bash test/lib/run.sh",
    "is_linted": "find . -name '*.js' | grep -v node_modules | grep -v coverage | xargs eslint",
    "coverage": "npm_config_coverage=y npm test && istanbul report html text-summary"
  },
  "keywords": [
    "load testing",
    "stress testing",
    "benchmark",
    "performance",
    "blackbox testing"
  ],
  "author": "Jongseok Choi <jseokchoi@smartm2m.co.kr>",
  "contributors": [
    "Jongseok Choi (jseokchoi@smartm2m.co.kr)",
    "Jonggyu Park (jonggyupark@smartm2m.co.kr)",
    "Sol Kang (sol@smartm2m.co.kr)",
    "Joowon Byun(joowonbyun@smartm2m.co.kr)"
  ],
  "license": "proprietary",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "http://gitlab.smartm2m.co.kr/pin/slippy.git"
  },
